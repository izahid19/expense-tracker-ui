{
  "rewrites": [{ "source": "/(.*)", "destination": "/" }],

  "headers": [
    {
      "source": "/(.*)\\.(js|mjs|jsx|css|png|jpg|jpeg|gif|svg|webp|woff2|woff|ttf|eot|json|txt)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=0, must-revalidate"
        }
      ]
    }
  ]
}